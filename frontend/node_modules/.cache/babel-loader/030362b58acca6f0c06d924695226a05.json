efault=\"{ active }\">\n                        <v-list-item-avatar color=\"grey darken-1\"></v-list-item-avatar>\n                    \n                        <v-list-item-content>\n                            <v-list-item-title>\n                                {{idx+1 }}\n                            </v-list-item-title>\n                            <v-list-item-subtitle>\n                                Id :  {{item.id }} * \n                                OrderId :  {{item.orderId }} * \n                                Price :  {{item.price }} * \n                                PayDate :  {{item.payDate }} * \n                                CancelDate :  {{item.cancelDate }} * \n                            </v-list-item-subtitle>\n                        </v-list-item-content>\n\n                        <v-list-item-action>\n                            <v-checkbox :input-value=\"active\" color=\"indigo\"></v-checkbox>\n                        </v-list-item-action>\n                    </template>\n                </v-list-item>\n            </v-list-item-group>\n        </v-list>\n    </div>\n</template>\n\n\n<script>\n    const axios = require('axios').default;\n\n    export default {\n        name: 'PaymentPicker',\n        props: {\n            value: [String, Object, Array, Number, Boolean],\n        },\n        data: () => ({\n            list: [],\n            selected: null,\n        }),\n        async created() {\n            var me = this;\n            var temp = await axios.get(axios.fixUrl('/payments'))\n            if(temp.data) {\n                me.list = temp.data._embedded.payments;\n            }\n\n            if(me.value && typeof me.value == \"object\") {\n                var id = Object.values(me.value)[0];\n                temp = await axios.get(axios.fixUrl('/payments/' + id))\n                if(temp.data) {\n                    var val = temp.data\n                    me.list.forEach(function(item, idx) {\n                        if(item.name == val.name) {\n                            me.selected = idx\n                        }\n                    })\n                }\n            }\n        },\n        methods: {\n            select(val) {\n                var obj = {}\n                if(val != undefined) {\n                    var arr = this.list[val]._links.self.href.split('/');\n                    obj['id'] = arr[4]; \n                    \n                    \n                    \n                    \n                    \n                    \n                    \n                    \n                    \n                    this.$emit('selected', obj);\n                }\n            },\n        },\n    };\n</script>\n\n\n"]}]}
enDialog\"\n          width=\"332.5\"\n          fullscreen\n          hide-overlay\n          transition=\"dialog-bottom-transition\"\n        >\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-fab-transition>\n                <v-btn\n                    color=\"blue\"\n                    fab\n                    dark\n                    large\n                    style=\"position:fixed; bottom: 5%; right: 2%; z-index:99\"\n                >\n                    <v-icon v-bind=\"attrs\" v-on=\"on\">mdi-plus</v-icon>\n                </v-btn>\n            </v-fab-transition>\n          </template>\n\n          <Payment :offline=\"offline\" class=\"video-card\" :isNew=\"true\" :editMode=\"true\" v-model=\"newValue\" @add=\"append\" v-if=\"tick\"/>\n\n          <v-btn\n                    style=\"postition:absolute; top:2%; right:2%\"\n                    @click=\"closeDialog()\"\n                    depressed \n                    icon \n                    absolute\n            >\n                    <v-icon>mdi-close</v-icon>\n          </v-btn>\n        </v-dialog>\n      </div>\n    </v-col>\n    <v-row>\n        <Payment :offline=\"offline\" class=\"video-card\" v-for=\"(value, index) in values\" v-model=\"values[index]\" v-bind:key=\"index\" @delete=\"remove\"/>\n    </v-row>\n  </div>\n</template>\n\n<script>\n\nconst axios = require('axios').default;\nimport Payment from './../Payment.vue';\n\nexport default {\n  name: 'PaymentManager',\n\n  components: {\n    Payment,\n  },\n\n  props: {\n      offline: Boolean\n  },\n\n  data: () => ({\n    values: [],\n    newValue: {},\n    tick : true,\n    openDialog : false,\n  }),\n\n  async created() {\n\n      if(this.offline){\n          if(!this.values) this.values = [];\n          return;\n      } \n\n      var temp = await axios.get(axios.fixUrl('/payments'))\n      this.values = temp.data._embedded.payments;\n\n  },\n\n  methods:{\n    closeDialog(){\n        this.openDialog = false\n    },\n    append(value){\n      this.tick = false\n      this.newValue = {}\n      this.values.push(value)\n      \n      this.$emit('input', this.values);\n\n      this.$nextTick(function(){\n        this.tick=true\n      })\n    },\n\n    remove(value){\n\n      var where = -1;\n      for(var i=0; i<this.values.length; i++){\n        if(this.values[i]._links.self.href == value._links.self.href){\n          where = i;\n          break;\n        }\n      }\n\n      if(where > -1){\n        this.values.splice(i, 1);\n        this.$emit('input', this.values);\n      }\n    }    \n  }\n};\n</script>\n\n\n<style>\n    .video-card {\n        width:300px; \n        margin-left:4.5%; \n        margin-top:50px; \n        margin-bottom:50px;\n    }\n</style>\n\n"]}]}
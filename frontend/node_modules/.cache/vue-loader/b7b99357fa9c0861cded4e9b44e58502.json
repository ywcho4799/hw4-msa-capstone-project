  color=\"blue\"\n                            fab\n                            dark\n                            large\n                            style=\"position:absolute; bottom: 5%; right: 2%; z-index:99\"\n                        >\n                            <v-icon v-bind=\"attrs\" v-on=\"on\">mdi-plus</v-icon>\n                        </v-btn>\n                    </v-fab-transition>\n                </template>\n\n                <Order :offline=\"offline\" class=\"video-card\" :isNew=\"true\" :editMode=\"true\" v-model=\"newValue\" @add=\"append\" v-if=\"tick\"/>\n                \n                <v-btn\n                    style=\"postition:absolute; top:2%; right:2%\"\n                    @click=\"closeDialog()\"\n                    depressed \n                    icon \n                    absolute\n                >\n                        <v-icon>mdi-close</v-icon>\n                </v-btn>\n                </v-dialog>\n            </div>\n        </v-col>\n    </div>\n</template>\n\n<script>\n  const axios = require('axios').default;\n  import Order from './../Order.vue';\n\n  export default {\n    name: 'OrderManager',\n    components: {\n        Order,\n    },\n    props: {\n      offline: Boolean,\n      editMode: Boolean,\n      isNew: Boolean\n    },\n    data: () => ({\n        values: [],\n        headers: [\n            { text: \"id\", value: \"id\" },\n            { text: \"menu\", value: \"menu\" },\n            { text: \"price\", value: \"price\" },\n            { text: \"customerId\", value: \"customerId\" },\n            { text: \"customerName\", value: \"customerName\" },\n            { text: \"customerTel\", value: \"customerTel\" },\n        ],\n        order : [],\n        newValue: {},\n        tick : true,\n        openDialog : false,\n    }),\n    async created() {\n      if(this.offline){\n          if(!this.values) this.values = [];\n          return;\n      } \n\n      var temp = await axios.get(axios.fixUrl('/orders'))\n      temp.data._embedded.orders.map(obj => obj.id=obj._links.self.href.split(\"/\")[obj._links.self.href.split(\"/\").length - 1])\n      this.values = temp.data._embedded.orders;\n\n    },\n    methods: {\n        closeDialog(){\n            this.openDialog = false\n        },\n        append(value){\n            this.tick = false\n            this.newValue = {}\n            this.values.push(value)\n            \n            this.$emit('input', this.values);\n\n            this.$nextTick(function(){\n                this.tick=true\n            })\n        },\n    }\n  }\n</script>\n\n"]}]}